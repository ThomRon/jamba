            "program": "${workspaceFolder}\\build\\bin\\Debug\\validator.exe",
            "args": ["${workspaceFolder}\\build\\VST3\\Debug\\[-target-].vst3"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
        },
        {
            "name": "Run Editor (Debug)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\build\\bin\\Debug\\editorhost.exe",
            "args": ["${workspaceFolder}\\build\\VST3\\Debug\\[-target-].vst3"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
        },

    ]
